{
  "version": 2,
  "builds": [
    {
      "src": "path/to/your/build/directory",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "**/*.prisma"
        ]
      },
      "buildEnv": {
        "PRISMA_FORCE_NAPI": "true"
      },
      "postbuild": "prisma generate"
    }
  ]
}